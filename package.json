{
  "name": "runway",
  "version": "0.1.4",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/philipahlberg/runway.git",
  "author": "philipahlberg <23071488+philipahlberg@users.noreply.github.com>",
  "license": "MIT",
  "keywords": [
    "router",
    "routing",
    "web components",
    "custom elements",
    "lazy-loading",
    "single-page application"
  ],
  "scripts": {
    "dev": "npm run test:watch",
    "test": "npm run build:test && npm run test:prod",
    "test:dev": "karma start scripts/karma.dev.js",
    "test:prod": "karma start scripts/karma.prod.js",
    "build": "npm run build:types && npm run build:prod",
    "build:types": "tsc --emitDeclarationOnly",
    "build:test": "rollup -c scripts/rollup.dev.js",
    "build:dev": "rollup -w -c scripts/rollup.dev.js",
    "build:prod": "rollup -c scripts/rollup.prod.js",
    "lint": "eslint src",
    "format": "prettier",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:build": "typedoc src/ --exclude \"**/constants.ts\" --out docs/",
    "docs:serve": "http-server ./docs",
    "example": "node scripts/example.js"
  },
  "devDependencies": {
    "@webcomponents/custom-elements": "^1.2.1",
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "event-target-shim": "^3.0.2",
    "http-server": "^0.11.1",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "koa": "^2.5.0",
    "koa-static": "^4.0.2",
    "mocha": "^5.0.4",
    "prettier": "^1.10.2",
    "rollup": "^0.56.2",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^3.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@philipahlberg/expressionist": "^0.1.2"
  }
}
