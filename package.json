{
  "name": "runway",
  "version": "0.2.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/philipahlberg/runway.git",
  "author": "philipahlberg <23071488+philipahlberg@users.noreply.github.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "router",
    "routing",
    "web components",
    "custom elements",
    "lazy-loading",
    "single-page application"
  ],
  "scripts": {
    "dev": "npm run test:watch",
    "test": "npm run build:test && npm run test:prod",
    "test:dev": "karma start scripts/karma.dev.js",
    "test:prod": "karma start scripts/karma.prod.js",
    "build": "npm run build:types && npm run build:prod",
    "build:types": "tsc --emitDeclarationOnly",
    "build:test": "rollup -c scripts/rollup.dev.js",
    "build:dev": "rollup -w -c scripts/rollup.dev.js",
    "build:prod": "rollup -c scripts/rollup.prod.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:build": "typedoc src/ --out docs/",
    "docs:serve": "node scripts/docs.js",
    "example": "node scripts/example.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "koa": "^2.7.0",
    "koa-static": "^5.0.0",
    "mocha": "^6.2.0",
    "prettier": "^1.19.1",
    "rollup": "^1.18.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.10.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "trailblazer": "^5.0.0"
  }
}
